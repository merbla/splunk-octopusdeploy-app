<dashboard script="autodiscover.js, deploymentDuration.js, tentacleHealth.js, activeUsers.js, avgDeploymentTime.js"
  stylesheet="octopusdeploy.css, nv.d3.min.css">
  <label>Deployments</label>
  <search id="durationGroupedSearch" ref="OctopusDeploy - DurationGrouped">

   </search>
  <row >
    <html>
      <div>
        Add some info/description
      </div>
    </html>
  </row>
  <row>
    <panel>
      <html>
        <h2>Punch Card</h2>
        <div id="punchCard">
            <svg id="punchCardChart"></svg>
        </div>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <h2>Tentacle Health</h2>
        <div id="byDuration">
            <svg id="byDurationChart"></svg>
        </div>
      </html>
    </panel>
    <panel>
      <html>
        <h2>Avgerage Deployment Time (seconds)</h2>
        <div id="avgDeploymentTime">
            <svg id="avgDeploymentTimeChart"></svg>
        </div>
      </html>
    </panel>
    <panel>
      <html>
        <h2>Active Users</h2>
        <div id="activeUsers">
          <svg id="activeUsersChart"></svg>
        </div>
      </html>
    </panel>
  </row>

  <row>
    <single id="avgDepSearchSingle">
      <search id="avgDepSearch" ref="OctopusDeploy - AvgDuration">
      </search>
    </single>
  </row>
</dashboard>
